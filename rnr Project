   "source": [
    "import requests\n",
    "\n",
    "# API key and headers\n",
    "api_key = 'ecHfSLzAporWWXs9HTkUARr08CZG7L9wCnVDGp29Xman554_aOV3E6ypR723fPZiLGDhb5sq35KA5oD_FZt_EGrcA5V6RdE_lDKM_qWED5tFQXPwzCTQNj81BdDvZXYx'\n",
    "headers = {'Authorization': 'Bearer %s' % api_key}\n",
    "\n",
    "# Yelp API endpoint and query parameters\n",
    "url = 'https://api.yelp.com/v3/businesses/search'\n",
    "params = {'term': 'coffee shops', 'location': 'Manhattan', 'limit': 50}\n",
    "\n",
    "# Make the request\n",
    "response = requests.get(url, headers=headers, params=params)\n",
    "\n",
    "# Check if the request was successful\n",
    "if response.status_code == 200:\n",
    "    # Parse the JSON response\n",
    "    data = response.json()\n",
    "\n",
    "    # Print each business's name and location\n",
    "    for business in data['businesses']:\n",
    "        print(f\"Name: {business['name']}\")\n",
    "        print(f\"Address: {', '.join(business['location']['display_address'])}\")\n",
    "        print('-' * 30)\n",
    "else:\n",
    "    print(f\"Error: Received response code {response.status_code}\")\n",
    "\n",
    "# Note: Yelp API does not provide review text in the business search endpoint\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "f5c716a6-a3ae-44f3-9189-a52f470a8226",
   "metadata": {},
   "outputs": [],
   "source": [
    "import requests\n",
    "\n",
    "# Your API Key\n",
    "api_key = 'your_api_key'\n",
    "headers = {'Authorization': f'Bearer {api_key}'}\n",
    "\n",
    "# Yelp API Endpoints\n",
    "search_url = 'https://api.yelp.com/v3/businesses/search'\n",
    "reviews_url = 'https://api.yelp.com/v3/businesses/{id}/reviews'\n",
    "\n",
    "# Search for coffee shops in Manhattan\n",
    "search_params = {'term': 'coffee shops', 'location': 'Manhattan', 'limit': 50}\n",
    "search_response = requests.get(search_url, headers=headers, params=search_params)\n",
    "businesses = search_response.json().get('businesses', [])\n",
    "\n",
    "# Filter businesses with reviews and get reviews for the first 10\n",
    "result_count = 0\n",
    "for business in businesses:\n",
    "    if result_count >= 10:\n",
    "        break\n",
    "\n",
    "    if business['review_count'] > 0:\n",
    "        business_id = business['id']\n",
    "        reviews_response = requests.get(reviews_url.format(id=business_id), headers=headers)\n",
    "        reviews_data = reviews_response.json()\n",
    "\n",
    "        print(f\"Business Name: {business['name']}\")\n",
    "        print(f\"Reviews: {reviews_data.get('reviews', [])}\")\n",
    "        print(\"\\n\")\n",
    "\n",
    "        result_count += 1\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "687bfb3c-f826-410d-be22-c66a7e12ccdb",
   "metadata": {},
   "outputs": [],
   "source": [
    "import requests\n",
    "\n",
    "# Your API Key\n",
    "api_key = 'your_api_key'\n",
    "headers = {'Authorization': f'Bearer {api_key}'}\n",
    "\n",
    "# Yelp API Endpoints\n",
    "search_url = 'https://api.yelp.com/v3/businesses/search'\n",
    "reviews_url = 'https://api.yelp.com/v3/businesses/{id}/reviews'\n",
    "\n",
    "# Search for coffee shops in Manhattan\n",
    "search_params = {'term': 'coffee shops', 'location': 'Manhattan', 'limit': 50}\n",
    "search_response = requests.get(search_url, headers=headers, params=search_params)\n",
    "businesses = search_response.json().get('businesses', [])\n",
    "\n",
    "# Filter businesses with reviews and get reviews for the first 10\n",
    "result_count = 0\n",
    "for business in businesses:\n",
    "    if result_count >= 10:\n",
    "        break\n",
    "\n",
    "    if business['review_count'] > 0:\n",
    "        business_id = business['id']\n",
    "        reviews_response = requests.get(reviews_url.format(id=business_id), headers=headers)\n",
    "        reviews_data = reviews_response.json()\n",
    "\n",
    "        print(f\"Business Name: {business['name']}\")\n",
    "        print(f\"Reviews: {reviews_data.get('reviews', [])}\")\n",
    "        print(\"\\n\")\n",
    "\n",
    "        result_count += 1\n"
   ]
  },
