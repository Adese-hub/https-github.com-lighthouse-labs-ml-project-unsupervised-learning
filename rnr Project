   ],
   "source": [
    "import pandas as pd\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "from sklearn.naive_bayes import MultinomialNB\n",
    "from sklearn.pipeline import make_pipeline\n",
    "\n",
    "# Sample data with added negative reviews\n",
    "data = {\n",
    "    'Business Name': [\n",
    "        'Zibetto Espresso Bar', \n",
    "        'Remi Flower & Coffee', \n",
    "        'Little Collins', \n",
    "        'Cafe Bad Experience', \n",
    "        'Dissatisfying Diner'\n",
    "    ],\n",
    "    'Review': [\n",
    "        'Good coffee. Food pastries. Good paninis.',\n",
    "        'Remi Flower & Coffee is a great place to grab coffee and sit down to chat.',\n",
    "        'This breakfast was the perfect way to wrap up our trip to NYC.',\n",
    "        'Terrible service, would not recommend.',\n",
    "        'Food was cold and unappetizing, very disappointed.'\n",
    "    ]\n",
    "}\n",
    "\n",
    "# Creating the DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Adding sentiment labels to the DataFrame\n",
    "# Assuming the first three reviews are positive (1) and the last two are negative (0)\n",
    "df['Sentiment'] = [1, 1, 1, 0, 0]\n",
    "\n",
    "# Example model (Naive Bayes Classifier)\n",
    "model = make_pipeline(CountVectorizer(), MultinomialNB())\n",
    "\n",
    "# Training the model with the DataFrame\n",
    "model.fit(df['Review'], df['Sentiment'])\n",
    "\n",
    "# Now, model can be used for predictions or further analysis\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "b6063cce-c63d-4443-b908-38fde6a4d28e",
   "metadata": {},
